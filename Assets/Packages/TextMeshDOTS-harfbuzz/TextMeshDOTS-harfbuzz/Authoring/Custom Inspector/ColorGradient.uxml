<UXML xmlns="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True">
    <Style src="project://database/Packages/com.textmeshdots/Authoring/ColorGradientStyle.uss?fileID=7433441132597879392&amp;guid=2452b2526412cf94ebc05320b710d496&amp;type=3#ColorGradientStyle" />
    <VisualElement data-source-type="TextMeshDOTS.TextMeshDOTSColorGradient, TextmeshDOTS" class="color-gradient-item" style="margin-top: 10px; margin-right: 10px; margin-bottom: 10px; margin-left: 10px;">
        <TextField label="Gradient name" placeholder-text="filler text" binding-path="name" />
        <EnumField label="Gradient Mode" value="HorizontalGradient" binding-path="colorMode" type="UnityEngine.TextCore.Text.ColorGradientMode, UnityEngine.TextCoreTextEngineModule" />
        <VisualElement name="Top" class="color-panel">
            <uie:ColorField label="TL" value="#00FFFFFF" binding-path="topLeft" name="Left" />
            <uie:ColorField label="TR" value="#00FFFFFF" binding-path="topRight" name="Right" />
        </VisualElement>
        <VisualElement name="Bottom" class="color-panel">
            <uie:ColorField label="BL" value="#00FFFFFF" binding-path="bottomLeft" name="Left" />
            <uie:ColorField label="BR" value="#00FFFFFF" binding-path="bottomRight" name="Right" />
        </VisualElement>
    </VisualElement>
</UXML>
